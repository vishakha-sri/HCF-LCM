import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        System.out.println("Enter two no.s:");
        int a = in.nextInt();
        int b = in.nextInt();
        int i,h=1,l;
        for(i=1;i<=a;i++){
            if(a%i==0 && b%i==0)
                h=i;
        }
        System.out.println("HCF is "+h);
        l=(a*b)/h;
        System.out.println("LCM is "+l);
    }
}
